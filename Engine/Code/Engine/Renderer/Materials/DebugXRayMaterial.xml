<Material name="DebugRenderXRay">
    <VertexShader  EngineShader="true" headerSrc="Shaders/" shaderName="Default_v" src="Shaders/Stages/Default_v.hlsl" entryPoint="VertexMain"/>
	<PixelShader   EngineShader="true" headerSrc="Shaders/" shaderName="Default_p" src="Shaders/Stages/Default_p.hlsl" entryPoint="PixelMain"/>
    <BlendMode value="Alpha"/>
    <!-- CCW - CounterClockwise
    CW - ClockWise -->
    <WindingOrder value="CCW"/> 
    <CullMode value="Back"/>
    <FillMode value="Solid"/>
    <Topology value="TriangleList"/>
    <DepthStencil depthFunction="ALWAYS" writeDepth="false"/>
    <Antialiasing value="8"/>
    <Metadata src="material.mtl"/>
</Material>